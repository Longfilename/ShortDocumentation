- var content = locals.content["header/_data.json"]
header.site-header
    div.row
        div.col-xs-1
            - if (content.logo) {
            a(href=content.logo.url)
                img(src=content.logo["image-src"], alt-text=content.logo["image-alt-text"])
            - }
        div.col-xs-11
            ul
                - (content.nav) && content.nav.map(function (link) {
                li
                    a(href=link.url)=link.text
                - })
