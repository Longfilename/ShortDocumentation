- var content = locals.content || [];
doctype html
html(lang="en")
    head
    
    body
        p default
        
        p=content.length
        ul
            - (content.length) && content.map(function (item) {
                li
                    a(href="./" + item.page,
                      data-json=item.json,
                      data-jade=item.jade,
                      data-js=item.js,
                      data-scss=item.scss,
                      data-md=item.md,
                      data-folder="../" + item.folder)=item.title
            - })
